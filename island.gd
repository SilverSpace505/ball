@tool
extends Node3D

@export var resolution: Vector3i
@export var isoLevel: float
@export var flatShaded: bool
@export var material: Material

@export_group("Generation")
@export var noiseSeed: int = 1
@export var noiseSize: float = 3

@export_group("Colours")
@export var grass: Color
@export var dirt: Color
@export var stone: Color

@export_group("Actions")
@export var regenerate: bool = false:
	set(value):
		if value and Engine.is_editor_hint():
			start_generate()
			#generate_vertices()
			regenerate = false

@export var randomise: bool = false:
	set(value):
		if value and Engine.is_editor_hint():
			noiseSeed = randi()
			start_generate()
			#generate_vertices()
			randomise = false

var thread: Thread

# Called when the node enters the scene tree for the first time.
func _ready() -> void:
	pass # Replace with function body.

# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta: float) -> void:
	pass
	
func _exit_tree() -> void:
	if thread.is_alive():
		thread.wait_to_finish()

func start_generate():
	thread = Thread.new()
	thread.start(generate_vertices)

func generate_vertices():
	
	var noise = FastNoiseLite.new()
	noise.seed = noiseSeed * 2
	noise.frequency = 0.05
	noise.noise_type = FastNoiseLite.TYPE_SIMPLEX
	
	var noise2 = FastNoiseLite.new()
	noise2.seed = noiseSeed * 2 + 1
	noise2.frequency = 0.1
	noise2.noise_type = FastNoiseLite.TYPE_SIMPLEX
	
	var grid = Grid.new(resolution)
	for x in range(grid.resolution.x):
		for y in range(grid.resolution.y):
			for z in range(grid.resolution.z):
				var p = Vector3(float(x) / grid.resolution.x, float(y) / grid.resolution.y, float(z) / grid.resolution.z) * 2.0 - Vector3(1.0, 1.0, 1.0)
				var n = noise.get_noise_3d(p.x * noiseSize, p.y * noiseSize, p.z * noiseSize)
				var n2 = noise2.get_noise_3d(p.x * noiseSize, p.y * noiseSize, p.z * noiseSize)
				var v = 0.75 * n2 - p.length() - max(p.y, 0) ** 2 * 2 + n * (1 - p.length())
				grid.write(x, y, z, -v, Color(p.x, p.y, p.z, 1))
	
	var vertices = PackedVector3Array()
	var colours = PackedVector4Array()
	for x in grid.resolution.x - 1:
		for y in grid.resolution.y - 1:
			for z in grid.resolution.z - 1:
				march_cube(x, y, z, grid, vertices, colours)
	#generate(vertices, colours)
	call_deferred('generate', vertices, colours)

func generate(vertices: PackedVector3Array, colours: PackedVector4Array):
	var surfaceTool = SurfaceTool.new()
	surfaceTool.begin(Mesh.PRIMITIVE_TRIANGLES)
	
	if flatShaded:
		surfaceTool.set_smooth_group(-1)
	
	for i in range(len(vertices)):
		surfaceTool.set_color(Color(colours[i].x, colours[i].y, colours[i].z, colours[i].w))
		surfaceTool.add_vertex(vertices[i])
	
	surfaceTool.generate_normals()
	surfaceTool.index()
	surfaceTool.set_material(material)
	$MeshInstance3D.mesh = surfaceTool.commit()
	$MeshInstance3D.position = Vector3(-resolution.x / 2.0, -resolution.y / 2.0, -resolution.z / 2.0)
	
	thread.wait_to_finish()
	
func march_cube(x: int, y: int, z: int, grid: Grid, vertices: PackedVector3Array, colours: PackedVector4Array):
	var tri = triangulate(x, y, z, grid)
	for edgeIndex in tri:
		if edgeIndex < 0: break
		var pointIndices = EDGES[edgeIndex]
		var p0 = POINTS[pointIndices.x]
		var p1 = POINTS[pointIndices.y]
		var pa = Vector3(x + p0.x, y + p0.y, z + p0.z)
		var pb = Vector3(x + p1.x, y + p1.y, z + p1.z)
		var pos = interpolate(pa, pb, grid)
		vertices.append(pos)
		var colour = interpolate_colour(pa, pb, grid)
		colours.append(Vector4(colour.r, colour.g, colour.b, colour.a))
	
func triangulate(x: int, y: int, z: int, grid: Grid):
	var idx = 0b00000000
	idx |= int(grid.read(x, y, z) < isoLevel) << 0
	idx |= int(grid.read(x, y, z + 1) < isoLevel) << 1
	idx |= int(grid.read(x + 1, y, z + 1) < isoLevel) << 2
	idx |= int(grid.read(x + 1, y, z) < isoLevel) << 3
	idx |= int(grid.read(x, y + 1, z) < isoLevel) << 4
	idx |= int(grid.read(x, y + 1, z + 1) < isoLevel) << 5
	idx |= int(grid.read(x + 1, y + 1, z + 1) < isoLevel) << 6
	idx |= int(grid.read(x + 1, y + 1, z) < isoLevel) << 7
	return TRIANGULATIONS[idx]

func interpolate(a: Vector3, b: Vector3, grid: Grid):
	var va = grid.read(a.x, a.y, a.z)
	var vb = grid.read(b.x, b.y, b.z)
	var t = (isoLevel - va) / (vb - va)
	return a + t * (b - a)
	
func interpolate_colour(a: Vector3, b: Vector3, grid: Grid):
	var va = grid.read(a.x, a.y, a.z)
	var vb = grid.read(b.x, b.y, b.z)
	var ca = grid.read_colour(a.x, a.y, a.z)
	var cb = grid.read_colour(b.x, b.y, b.z)
	var t = (isoLevel - va) / (vb - va)
	return ca + t * (cb - ca)

class Grid:
	var data: PackedFloat32Array
	var colours: PackedFloat32Array
	var resolution: Vector3i
	
	func _init(res: Vector3i):
		self.resolution = res
		self.data.resize(resolution.x * resolution.y * resolution.z)
		self.data.fill(1.0)
		
		self.colours.resize(resolution.x * resolution.y * resolution.z * 4)
		self.colours.fill(1.0)
	
	func read(x: int, y: int, z: int):
		return self.data[x * self.resolution.y * self.resolution.z + y * self.resolution.z + z]
	
	func read_colour(x: int, y: int, z: int):
		var r = self.colours[(x * self.resolution.y * self.resolution.z + y * self.resolution.z + z) * 4 + 0]
		var g = self.colours[(x * self.resolution.y * self.resolution.z + y * self.resolution.z + z) * 4 + 1]
		var b = self.colours[(x * self.resolution.y * self.resolution.z + y * self.resolution.z + z) * 4 + 2]
		var a = self.colours[(x * self.resolution.y * self.resolution.z + y * self.resolution.z + z) * 4 + 3]
		return Color(r, g, b, a)
	
	func write(x: int, y: int, z: int, v: float, c: Color):
		self.data[x * self.resolution.y * self.resolution.z + y * self.resolution.z + z] = v
		self.colours[(x * self.resolution.y * self.resolution.z + y * self.resolution.z + z) * 4 + 0] = c.r
		self.colours[(x * self.resolution.y * self.resolution.z + y * self.resolution.z + z) * 4 + 1] = c.g
		self.colours[(x * self.resolution.y * self.resolution.z + y * self.resolution.z + z) * 4 + 2] = c.b
		self.colours[(x * self.resolution.y * self.resolution.z + y * self.resolution.z + z) * 4 + 3] = c.a

